    "embed": {
      "include": [ "Resources/**/*" ]
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.5.0-rc2-*",
    "xunit": "2.1.0-*",
    "dotnet-test-xunit": {
      "version": "1.0.0-rc2-*",
      "target": "package"
    },
    "FubarCoder.RestSharp.Portable.HttpClient": {
      "target": "project",
      "version": ""
