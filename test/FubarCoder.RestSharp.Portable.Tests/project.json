    "FubarCoder.RestSharp.Portable.WebRequest": {
      "target": "project",
      "version": ""
    },
    "FubarCoder.RestSharp.Portable.OAuth1": {
      "target": "project",
      "version": ""
    },
    "BouncyCastle": "*",
