{
  "name": "FubarCoder.RestSharp.Portable.Encodings",
  "version": "4.0.0-alpha1-*",
  "title": "RestSharp.Portable Encodings",
  "authors": [ "Mark Junker", "Fubar Development Junker" ],
  "description": "This library contains the GZIP and Deflate content encoding handlers.\n\nSupported Platforms:\n- .NET Standard 1.0\n- .NET Framework 4",
  "copyright": "Copyright Â© RestSharp.Portable project 2013-2016",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "debugType": "none",
        "xmlDoc": true
      }
    }
  },
  "buildOptions": {
    "keyFile": "../../RestSharp.Portable.snk",
    "compile": [ "../../GlobalAssemblyInfo.cs" ]
  },
  "packOptions": {
    "summary": "Library that will contain some standard Content-Encoding handlers",
    "owners": [ "FubarCoder" ],
    "licenseUrl": "http://choosealicense.com/licenses/bsd-2-clause/",
    "projectUrl": "https://github.com/FubarDevelopment/restsharp.portable",
    "tags": [ "PCL", "RestSharp", "portable", "core" ],
    "releaseNotes": "- Using the new core library",
    "repository": {
      "type": "git",
      "url": "https://github.com/FubarDevelopment/restsharp.portable"
    },
    "files": {
      "mappings": {
        "/": "../../*.md"
      }
    }
  },
  "dependencies": {
    "JetBrains.Annotations": {
      "version": "*",
      "type": "build"
    },
    "FubarCoder.RestSharp.Portable.Core": {
      "target": "project",
      "version": ""
    }
  },
  "frameworks": {
    "netstandard1.0": {
      "imports": [ "dnxcore50" ],
      "dependencies": {
        "Zlib.Portable": {
          "target": "project",
          "type": "build"
        }
      }
    },
    "netstandard1.1": {
      "imports": [ "dnxcore50" ],
      "dependencies": {
        "System.IO.Compression": "4.1.0-rc2-*"
      }
    },
    "net40": {
    }
  }
}
