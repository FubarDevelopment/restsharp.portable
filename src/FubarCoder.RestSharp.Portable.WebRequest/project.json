{
  "name": "FubarCoder.RestSharp.Portable.WebRequest",
  "version": "4.0.0-*",
  "title": "RestSharp.Portable HttpWebRequest engine",
  "authors": [ "Mark Junker", "Fubar Development Junker" ],
  "description": "Some kind of a RestSharp port to PCL. This is not related to the RestSharp project at all!\n\nSupported Platforms:\n- .NET Standard 1.1\n- .NET Framework 4\n- Silverlight 5\n- Portable Class Libraries (Profiles 259 and 328)",
  "copyright": "Copyright © RestSharp.Portable project 2013-2016",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "debugType": "none",
        "xmlDoc": true,
        "optimize": true
      }
    }
  },
  "buildOptions": {
    "keyFile": "../../RestSharp.Portable.snk",
    "compile": [ "../../GlobalAssemblyInfo.cs" ]
  },
  "packOptions": {
    "summary": "Some kind of a RestSharp port to PCL",
    "owners": [ "FubarCoder" ],
    "licenseUrl": "http://choosealicense.com/licenses/bsd-2-clause/",
    "projectUrl": "https://github.com/FubarDevelopment/restsharp.portable",
    "tags": [ "PCL", "RestSharp", "portable", "WebRequest", "HttpWebRequest" ],
    "releaseNotes": "- Using the new core library\n- Treat Xamarin platform the same way as the .NET Framework for HttpWebRequest properties like ContentLength (issue 95)",
    "repository": {
      "type": "git",
      "url": "https://github.com/FubarDevelopment/restsharp.portable"
    },
    "files": {
      "mappings": {
        "/": "../../*.md",
        "lib/native/": "lib/native/_._",
        "tools/native/": "tools/native/*.ps1"
      }
    }
  },
  "dependencies": {
    "JetBrains.Annotations": {
      "version": "*",
      "type": "build"
    },
    "FubarCoder.RestSharp.Portable.Core": {
      "target": "project",
      "version": ""
    }
  },
  "frameworks": {
    ".NETPortable,Version=v4.0,Profile=Profile328": {
      "buildOptions": {
        "compile": [
          "../../GlobalAssemblyInfo.cs"
        ],
        "define": [ "PROFILE328", "NO_PROXY", "USE_TASKEX" ]
      },
      "dependencies": {
        "Microsoft.Bcl.Async": "1.0.168",
        "FubarCoder.RestSharp.Portable.Encodings": {
          "target": "project",
          "version": ""
        }
      },
      "frameworkAssemblies": {
        "mscorlib": { "type": "build" },
        "System": { "type": "build" },
        "System.Net": ""
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "buildOptions": {
        "compile": [
          "../../GlobalAssemblyInfo.cs"
        ],
        "define": [ "PROFILE259", "NO_PROXY" ]
      },
      "dependencies": {
        "FubarCoder.RestSharp.Portable.Encodings": {
          "target": "project",
          "version": ""
        }
      },
      "frameworkAssemblies": {
        "mscorlib": { "type": "build" },
        "System": { "type": "build" },
        "System.Net.Requests": ""
      }
    },
    "netstandard1.1": {
      "buildOptions": {
        "additionalArguments": [ "/ruleset:../../ReleaseRules.ruleset", "/additionalfile:../../stylecop.json" ]
      },
      "imports": [ "dotnet" ],
      "dependencies": {
        "System.Net.Requests": "4.0.11",
        "FubarCoder.RestSharp.Portable.Encodings": {
          "target": "project",
          "version": ""
        },
        "StyleCop.Analyzers": {
          "type": "build",
          "version": "1.0.0"
        }
      }
    },
    "net40": {
      "buildOptions": {
        "define": [ "USE_TASKEX" ]
      },
      "dependencies": {
        "Microsoft.Bcl.Async": "1.0.168"
      }
    },
    "net45": {
    },
    "sl5": {
      "buildOptions": {
        "define": [ "USE_TASKEX", "NO_PROXY" ]
      },
      "dependencies": {
        "Microsoft.Bcl.Async": "1.0.168"
      }
    },
    "MonoAndroid10": {
      "frameworkAssemblies": {
        "mscorlib": { "type": "build" },
        "System": { "type": "build" },
        "System.Core": { "type": "build" },
        "System.Net": ""
      }
    },
    "MonoTouch10": {
      "frameworkAssemblies": {
        "mscorlib": { "type": "build" },
        "System": { "type": "build" },
        "System.Core": { "type": "build" },
        "System.Net": ""
      }
    },
    "Xamarin.iOS10": {
      "frameworkAssemblies": {
        "mscorlib": { "type": "build" },
        "System": { "type": "build" },
        "System.Core": { "type": "build" },
        "System.Net": ""
      }
    }
  }
}
