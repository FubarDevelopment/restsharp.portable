          </group>
          <group targetFramework="MonoAndroid10">
            <dependency id="FubarCoder.RestSharp.Portable" version="$clientversion$" />
            <dependency id="FubarCoder.RestSharp.Portable.OAuth" version="$clientversion$" />
          </group>
          <group targetFramework="MonoTouch10">
            <dependency id="FubarCoder.RestSharp.Portable" version="$clientversion$" />
            <dependency id="FubarCoder.RestSharp.Portable.OAuth" version="$clientversion$" />
          </group>
          <group targetFramework="Xamarin.iOS10">
            <dependency id="FubarCoder.RestSharp.Portable" version="$clientversion$" />
            <dependency id="FubarCoder.RestSharp.Portable.OAuth" version="$clientversion$" />
          </group>
        </dependencies>
    </metadata>
